<P align=center><B><U>Queue(Send) Email Template</U></B></P>

<SCRIPT LANGUAGE="JavaScript">

var QueryString = "<!-- #echo var="_QUERY_STR" -->";
var _ErrorCode = -1
_Index1 = (QueryString.length);
_Index2 = (QueryString.indexOf("err="))+4;
if (_Index2==4) {
	_ErrorCode = QueryString.substring(_Index2,_Index1);
}
if (_Index2<4) {
	_ErrorCode = 999;
}
if (_ErrorCode==0) {
	document.write (" Email has been successfully placed in the queue.  The email will automatically be sent within the next 2 minutes based on network issues.  If the email is not sent successfully an alarm will be posted.");
}
if (_ErrorCode>0) {
	 document.write ("<P><font color=red><B>ERROR:</B> Email not sent to queue. Review specific error below to identify why this email could not be sent.</font><P>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;");
}
if (_ErrorCode==1) {
	 document.write (" ROS packet error--request not sent to SMTP task.");
}

if (_ErrorCode==2) {
	 document.write (" The name value pairs passed in did not make sense.");
}

if (_ErrorCode==3) {
	 document.write (" DRC Agent is disabled in sysvars.");
}

if (_ErrorCode==4) {
	 document.write (" Illegal access--tried to get around cookies.");
}

if (_ErrorCode==5) {
	 document.write (" Cookies may not be enabled on browser.");
}

if (_ErrorCode==6) {
	 document.write (" Error in attempt to modify xml file.");
}

if (_ErrorCode==7) {
	 document.write (" The DRC advanced option is not loaded.");
}
   

if (_ErrorCode==999) {
	 document.write (" Uninitialized error code from DRC Agent.");
}

</SCRIPT>


